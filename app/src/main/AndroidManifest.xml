<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fx50j.keychaindemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



        <activity android:name=".Add_Activity"
            android:label="@string/add_name"
            android:parentActivityName=".MainActivity">
        </activity>

        <activity android:name=".Item_Activity"
            android:label="keychain"
            android:parentActivityName=".MainActivity"
            android:launchMode="singleTop">
        </activity>

        <activity android:name=".Edit_Activity"
            android:parentActivityName=".Item_Activity"
            android:label="编辑">
        </activity>
    </application>

</manifest>
